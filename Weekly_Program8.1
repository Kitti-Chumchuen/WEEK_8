#include <stdio.h>

int main(){
  int num[10];
  int a = 0;
  printf("*** Enter 10 numbers ***\n\n");
  do{
    printf("%d) Enter a number : ",a+1);
    scanf("%d", &num[a]);
    a++;
  } while(a<10);
  printf("\n");
  printf("Data in array : ");
  for(int i=0; i<10; i++){
    printf(" %d", num[i]);
  }
  printf("\n");
  printf ("Result : ");
  for(int i=0; i<10; i++){
    if(num[i-1]%2 != 0 && num[i+1]%2 != 0){
      printf(" %d", num[i]);
    }
  }
    return 0;
}
